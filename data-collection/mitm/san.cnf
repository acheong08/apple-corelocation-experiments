[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = dn
req_extensions     = v3_req

[ dn ]
C  = US
ST = California
L  = Cupertino
O  = Apple Inc
CN = *.ls.apple.com

[ v3_req ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = *.ls.apple.com
DNS.2 = gs-loc.apple.com
